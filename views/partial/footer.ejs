</div>

<script>
  function myAccFunc() {
    var x = document.getElementById("demoAcc");
    if (x.className.indexOf("w3-show") == -1) {
      x.className += " w3-show";
      x.previousElementSibling.className += " w3-green";
    } else {
      x.className = x.className.replace(" w3-show", "");
      x.previousElementSibling.className =
        x.previousElementSibling.className.replace(" w3-green", "");
    }
  }

  function myDropFunc() {
    var x = document.getElementById("demoDrop");
    if (x.className.indexOf("w3-show") == -1) {
      x.className += " w3-show";
      x.previousElementSibling.className += " w3-green";
    } else {
      x.className = x.className.replace(" w3-show", "");
      x.previousElementSibling.className =
        x.previousElementSibling.className.replace(" w3-green", "");
    }
  }

  function w3_open() {
    document.getElementById("mySidebar").style.display = "block";
    document.getElementById("fullpage").style.display = "inline";
    document.getElementById("fullpage").style.marginLeft = "auto";
    document.getElementById("fullpage").style.marginTop = "0";
    document.getElementById("fullpage").style.paddingLeft = "auto";
  }

  function w3_close() {
    document.getElementById("mySidebar").style.display = "none";
    document.getElementById("fullpage").style.margin = "auto";
    document.getElementById("fullpage").style.display = "block";
  }

</script>

